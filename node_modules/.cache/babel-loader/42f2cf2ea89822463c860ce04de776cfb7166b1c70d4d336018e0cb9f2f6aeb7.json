orage.getItem(\"user\")).userName;\n\n      const response = await axios.get(\n        `${\n          process.env.NODE_ENV === \"production\"\n            ? \"http://127.0.0.1:9000\"\n            : \"http://127.0.0.1:9000\"\n        }/user/historyLog`,\n        {\n          params: {\n            username: this.username,\n          },\n        }\n      );\n      this.historyLog = response.data;\n    } catch (error) {\n      console.error(\"Error fetching history log:\", error);\n    }\n  },\n  methods: {\n    formatDate(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString();\n    },\n  },\n};\n</script>\n\n<style scoped>\n.history-log {\n  margin-top: 20px;\n}\n\n.log-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.log-table th,\n.log-table td {\n  padding: 8px;\n  border-bottom: 1px solid #ddd;\n}\n\n.log-table th {\n  background-color: #f2f2f2;\n}\n\n.even-row {\n  background-color: #f9f9f9;\n}\n</style>\n"],"mappings":"AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,IAAI,CAACD,QAAO,GAAIE,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC,CAACC,QAAQ;MAEjE,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAC7B,GACCC,OAAO,CAACC,GAAG,CAACC,QAAO,KAAM,YAAW,GAChC,uBAAsB,GACtB,uBACL,kBAAiB,EAClB;QACEC,MAAM,EAAE;UACNZ,QAAQ,EAAE,IAAI,CAACA;QACjB;MACF,CACF,CAAC;MACD,IAAI,CAACD,UAAS,GAAIQ,QAAQ,CAACT,IAAI;IACjC,EAAE,OAAOe,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;IACrD;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,UAAUA,CAACC,SAAS,EAAE;MACpB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,SAAS,CAAC;MAChC,OAAOC,IAAI,CAACE,cAAc,CAAC,CAAC;IAC9B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}